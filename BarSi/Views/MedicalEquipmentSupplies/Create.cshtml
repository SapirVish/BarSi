@model BarSi.Models.MedicalEquipmentSupply

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>MedicalEquipmentSupply</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Hospital" class="control-label"></label>
                <select asp-for="HospitalId" class ="form-control" asp-items="ViewBag.HospitalId"></select>
            </div>
            <div class="form-group">
                <label asp-for="MedicalEquipment" class="control-label"></label>
                <select asp-for="MedicalEquipmentId" class ="form-control" asp-items="ViewBag.MedicalEquipmentId"></select>
            </div>
            <div class="form-group">
                <label asp-for="SupplyQuantity" class="control-label"></label>
                <input asp-for="SupplyQuantity" class="form-control" />
                <span asp-validation-for="SupplyQuantity" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-outline-info" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
